{
	"name": "jlbelanger/magnolia",
	"description": "Recipes.",
	"version": "1.0.0",
	"type": "project",
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Jenny Belanger",
			"email": "mail@jennybelanger.com",
			"homepage": "https://www.jennybelanger.com/"
		}
	],
	"require": {
		"php": "^8.4",
		"erusev/parsedown": "^1.7.4",
		"guzzlehttp/guzzle": "^7.10.0",
		"laravel/framework": "^12.43.1",
		"laravel/sanctum": "^4.2.1",
		"laravel/tinker": "^2.10.2"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.16.2",
		"fakerphp/faker": "^1.24.1",
		"filp/whoops": "^2.18.4",
		"mockery/mockery": "^1.6.12",
		"nunomaduro/collision": "^8.8.3",
		"phpunit/phpunit": "^12.5.4",
		"squizlabs/php_codesniffer": "^4.0.1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"discard-changes": true,
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": ["barryvdh/laravel-debugbar"]
		}
	}
}
